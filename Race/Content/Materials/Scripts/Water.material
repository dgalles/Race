material Water/Water1
{
	technique
	{
		pass
		{
			scene_blend colour_blend
			depth_write off

			texture_unit
			{
				texture BeachStones.jpg
				scale 0.25 0.25
			}

		}
	}
}

material Water/Water2
{
	technique
	{
		pass
		{
			lighting off

			texture_unit
			{
				cubic_texture cloudy_noon.jpg combinedUVW
				tex_address_mode clamp
				env_map cubic_reflection
			}
		}
	}
}
